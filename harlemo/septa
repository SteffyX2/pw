#include <stdio.h>
#include <stdlib.h>
 int kvadrat_kub(int n, int* kv, int* kb)
{
    *kv=n*n;
    *kb=n*n*n;
}

 int main()
{
    int n,m,i,kv,kb;
    printf("Unesite n i m:");
    scanf("%d%d", &n, &m);
    for(i=n;i<=m;i++)
    {
    kvadrat_kub(i,&kv,&kb);
    printf("\nBroj:%d, Kvadrat:%d, Kub:%d, Suma:%d",i,kv,kb,kv+kb);
    }
}
